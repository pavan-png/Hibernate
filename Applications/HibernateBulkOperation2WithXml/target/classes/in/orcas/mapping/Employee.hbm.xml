<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.orcas.model">

    <class name="Employee" table="emp_new">
        
        <id name="eNo" column="eno">
            <generator class="identity"/>
        </id>

        <property name="eName" column="ename" length="20"/>
        <property name="eAddr" column="eaddr" length="20"/>
        <property name="eSalary" column="esal"/>

    </class>
</hibernate-mapping>
